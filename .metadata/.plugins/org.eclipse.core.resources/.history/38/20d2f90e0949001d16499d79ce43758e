package iArrays;

import java.util.Scanner;

public class BFindLargestElementOfArray {

	public static int[] inputArr() {
		Scanner s = new Scanner(System.in);
		
		int size = s.nextInt();
		int[] arr = new int[size];
		for(int i=0;i<size;i++) {
			arr[i] = s.nextInt();
		}
		return arr;
	}
	public static void main(String[] args) {
		
	}
}
