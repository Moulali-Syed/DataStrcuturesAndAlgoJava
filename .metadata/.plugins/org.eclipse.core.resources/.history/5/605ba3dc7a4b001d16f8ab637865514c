package kStrings;

import java.util.Scanner;

public class NReverseEachWord {

	public static String reverseStr(String st) {
		String revStr = "";
		for(int i=st.length()-1;i>=0;i--) {
			revStr+=st.charAt(i);
		}
	}
	public static void reverseEachWord() {
