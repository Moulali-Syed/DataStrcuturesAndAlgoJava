package ePatterns;

import java.util.Scanner;

public class BSquarePattern {

	Scanner s = new Scanner(System.in);
	public void squarePattern1() {
		int n = s.nextInt();
		
		int i=1;
		while(i<=n) {
			int j=1;
			while(j<=n) {
				System.out.print("*");
				j++;
			}
			System.out.println();
			i++;
		}
	}
}
