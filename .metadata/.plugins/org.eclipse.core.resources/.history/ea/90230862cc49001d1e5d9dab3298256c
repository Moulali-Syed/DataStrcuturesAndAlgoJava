package iArrays;
/*
Integers store all its values continuosly
index starts from 0
 we can find array lenght using arrName.length
 
 */
public class AArrayBasics {
	
	public static void main(String[] args) {
		//declare an array
		int[] arr = new int[10];
		
		arr[0] = 5;
		System.out.println(arr[0]);//5
	}

}
