package dRelationalLogicalOperators;

public class HNestedif {

	public static void main(String[] args) {
		int a = 10;
		int b = 2;
		if (a > b) {
			System.out.println(" a is greater");
		} else if (a == b) {
			System.out.println(" a and b are equal");
		} else {
			System.out.println("b is greater");
		}

	}
}
