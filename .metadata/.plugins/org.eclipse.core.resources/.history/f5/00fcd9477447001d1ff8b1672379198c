package ePatterns;

import java.util.Scanner;

public class ETriangularPattern {

	Scanner s = new Scanner(System.in);
	public void triangularPattern1() {
		int n = s.nextInt();
		
		int i=1;
		while(i<=n) {
			int j=1;
			while(j<=i) {
				System.out.print(j);
				j++;
			}
			System.out.println();
			i++;
		}
	}
}
