package kStrings;

public class SRemoveCharacter {

	public static String removeChar(String str,char c) {
		
		String st = "";
		for(int i=0;i<str.length();i++) {
			if(str.charAt(i)==c) {
				continue;
			}else {
				st+=str.charAt(i);
			}
		}
		return st;
	}
	
	public static void main(String[] args) {
		String s = removeChar("aabbbjesajja",'a');
		System.out.println(s);
	}
}
