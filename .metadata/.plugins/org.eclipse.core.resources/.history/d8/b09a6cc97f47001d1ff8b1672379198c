package ePatterns;

import java.util.Scanner;

public class ICharacterPattern {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		for(int i=1;i<=n;i++) {
			int ch = 'A';
			for(int j=1;j<=n;j++) {
				System.out.print((char)ch);
				ch++;
			}
			System.out.println();
		}
	}
}
//A-  65[ASCII]