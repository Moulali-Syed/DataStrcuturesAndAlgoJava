package iArrays;

public class LHowNonPrimitveStored {

	public static void incrementArr(int[] arr) {
		for(int i=0;i<arr.length;i++) {
			arr[i] = arr[i]+1;
		}
	}
	public static void main(String[] args) {
		int[] arr = {1,2,3,4,5};
		System.out.println(arr);//[I@515f550a
		incrementArr(arr);
		System.out.println(arr);//[I@515f550a
		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i]+" ");
		}
	}
}
