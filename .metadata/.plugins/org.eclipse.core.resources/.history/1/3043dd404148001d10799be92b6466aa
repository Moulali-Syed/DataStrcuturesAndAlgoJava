package gOperatorsAndForloops;

public class GPrecedenceAndAssociativity {

	public static void main(String[] args) {
		/*
operators 			precedence
postfix 			expr++,expr--
unary				++expr,--expr , +expr, -expr , ~ !
multiplicative 		/, * , %
additive			+,-
shift 				<< >>  >>>
relational			<  >  <=  >=  instanceof
equality			== !=
bitwiseAND 			&
bitwiseXOR 			^
bitwise OR			|
logicalAND			&&
logicalOR			||
ternary				? :
assignment			+=,-=,*=,/=,%=, &= , |= ,>>= , <<=,>>>=
		 */
	}
}
//>>> - unsigned right shift , sign will not be copied